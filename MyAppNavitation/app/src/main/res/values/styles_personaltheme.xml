<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="AutoCompleteTextViewPersonalTheme" parent="android:Widget.Holo.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/personaltheme_list_selector_holo_dark</item>
	    <item name="android:background">@drawable/personaltheme_edit_text_holo_dark</item>
  </style>

  <style name="ButtonPersonalTheme" parent="android:Widget.Holo.Button">
	  <item name="android:background">@drawable/personaltheme_btn_default_holo_dark</item>
  </style>

  <style name="ImageButtonPersonalTheme" parent="android:Widget.Holo.ImageButton">
	  <item name="android:background">@drawable/personaltheme_btn_default_holo_dark</item>
  </style>

  <style name="SpinnerPersonalTheme" parent="android:Widget.Holo.Spinner">
      <item name="android:background">@drawable/personaltheme_spinner_background_holo_dark</item>
      <item name="android:dropDownSelector">@drawable/personaltheme_list_selector_holo_dark</item>
  </style>

  <style name="TabPersonalTheme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/personaltheme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextPersonalTheme">
	  <item name="android:textColor">#ffffff</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarPersonalTheme" parent="android:Widget.Holo.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/personaltheme_progress_horizontal_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/personaltheme_progress_indeterminate_horizontal_holo_dark</item>
  </style>

  <style name="SeekBarPersonalTheme" parent="android:Widget.Holo.SeekBar">
      <item name="android:progressDrawable">@drawable/personaltheme_scrubber_progress_horizontal_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/personaltheme_scrubber_progress_horizontal_holo_dark</item>
      <item name="android:thumb">@drawable/personaltheme_scrubber_control_selector_holo_dark</item>
  </style>

  <style name="RatingBarPersonalTheme" parent="android:Widget.Holo.RatingBar">
      <item name="android:progressDrawable">@drawable/personaltheme_ratingbar_full_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/personaltheme_ratingbar_full_holo_dark</item>
  </style>

  <style name="RatingBarBigPersonalTheme" parent="android:Widget.Holo.RatingBar.Indicator">
      <item name="android:progressDrawable">@drawable/personaltheme_ratingbar_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/personaltheme_ratingbar_holo_dark</item>
  </style>

  <style name="RatingBarSmallPersonalTheme" parent="android:Widget.Holo.RatingBar.Small">
      <item name="android:progressDrawable">@drawable/personaltheme_ratingbar_small_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/personaltheme_ratingbar_small_holo_dark</item>
